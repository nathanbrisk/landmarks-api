    "paths": {
        "/landmarks-api/canadian-provinces": {
            "get": {
                "description": "Return all Canadian provinces",
                "operationId": "App\\Http\\Controllers\\CanadianProvincesController::showAll",
                "responses": {
                    "200": {
                        "description": "successful operation",
                        "content": {
                            "application/json": {}
                        }
                    },
                    "500": {
                        "description": "server error",
                        "content": {
                            "application/json": {}
                        }
                    }
                }
            }
        },
        "/landmarks-api/canadian-provinces/{slug}": {
            "get": {
                "description": "Return a single Canadian province by slug",
                "operationId": "App\\Http\\Controllers\\CanadianProvincesController::showOne",
                "parameters": [
                    {
                        "name": "slug",
                        "in": "path",
                        "description": "Slug of Canadian province to return",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "successful operation",
                        "content": {
                            "application/json": {}
                        }
                    },
                    "400": {
                        "description": "Invalid slug supplied"
                    },
                    "404": {
                        "description": "Canadian province not found"
                    }
                }
            }
        },
